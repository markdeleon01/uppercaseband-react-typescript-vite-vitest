{
  "name": "uppercaseband-react-typescript-vite-vitest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest --run --passWithNoTests",
    "coverage": "vitest run --coverage",
    "cy:open-unit": "npx cypress open --component --browser chrome",
    "cy:open-e2e": "npx cypress open --e2e --browser chrome",
    "cy:run-unit": "npx cypress run --component --headless --record --key 250b7273-d100-471d-be8d-f436b2a6bfd9",
    "cy:run-e2e": "npx cypress run --e2e --headless --record --key 250b7273-d100-471d-be8d-f436b2a6bfd9",
    "cy:e2e": "start-server-and-test start http-get://localhost:5173 cy:run-e2e",
    "install:xvfb": "sudo apt-get update && sudo apt-get install libgtk2.0-0 && sudo apt-get install libgtk-3-0 && sudo apt-get install libgbm-dev && sudo apt-get install libnotify-dev && sudo apt-get install libgconf-2-4 && sudo apt-get install libnss3 && sudo apt-get install libxss1 && sudo apt-get install libasound2 && sudo apt-get install libxtst6 && sudo apt-get install xauth && sudo apt-get install xvfb",
    "make-badges": "istanbul-badges-readme",
    "make-badges:ci": "npm run make-badges",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.5",
    "html-react-parser": "^5.1.10",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "redux-mock-store": "^1.5.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-html-parser": "^2.0.6",
    "@types/redux-mock-store": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^1.6.0",
    "@vitest/coverage-v8": "^1.6.0",
    "cypress": "^13.10.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "istanbul-badges-readme": "^1.9.0",
    "jsdom": "^24.1.0",
    "start-server-and-test": "^2.0.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-istanbul": "^6.0.2",
    "vitest": "^1.6.0"
  }
}
